<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Presentacion.MainActivity">

    <com.mitiempo.pruebaolimpiaitandroid.Presentacion.Pantalla4.EstadosBluetoothWifi
        android:id="@+id/verificador_estados_bluetooth_wifi"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mitiempo.pruebaolimpiaitandroid.Presentacion.Pantalla3.PosicionGeografica
        android:id="@+id/pocision_geografica"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/selector_foto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.mitiempo.pruebaolimpiaitandroid.Presentacion.Pantalla2.SeleccionarFoto
        android:id="@+id/selector_foto"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

    </com.mitiempo.pruebaolimpiaitandroid.Presentacion.Pantalla2.SeleccionarFoto>
</androidx.constraintlayout.widget.ConstraintLayout>